<%= form_for song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist%>
  <%= f.collection_select :artist_id, Artist.all, :id, :name, include_blank: true %> <br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.submit %>
<% end %>
